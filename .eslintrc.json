{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": 0,
        "react/prop-types": 0,
        "react/prefer-stateless-function": 0
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "document": false
    }
}