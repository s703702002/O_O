{
  "name": "o_o",
  "version": "1.0.0",
  "description": "funny",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint **/*.js",
    "dev": "nodemon",
    "build:server": "babel src/ --out-dir build",
    "start": "NODE_ENV=production node ./build/server.js",
    "preproduction": "rm -rf build/",
    "production": "npm run build:server && webpack --process --config webpack.prod.js && npm run copy:static",
    "copy:static": "cp src/api/*.json build/api/ && mkdir -p build/static && cp -R src/static/* build/static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s703702002/O_O.git"
  },
  "author": "stanley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s703702002/O_O/issues"
  },
  "homepage": "https://github.com/s703702002/O_O#readme",
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "css-loader": "^2.1.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.4"
