{
  "name": "o_o",
  "version": "1.0.0",
  "description": "funny",
  "main": "./build/server.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "dev": "nodemon",
    "build:server": "babel src/ --out-dir build --ignore src/**/*.spec.js,src/**/*.test.js",
    "start": "NODE_ENV=production node ./build/server.js",
    "preproduction": "rm -rf build/",
    "production": "npm run build:server && webpack --process --config webpack.prod.js && npm run copy:static",
    "copy:static": "cp src/api/*.json build/api/ && mkdir -p build/static && cp -R src/static/* build/static",
    "functional:local": "codeceptjs run --verbose --steps --config=./codecept/codecept.local.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s703702002/O_O.git"
  },
  "author": "stanley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/s703702002/O_O/issues"
  },
  "homepage": "https://github.com/s703702002/O_O#readme",
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/__e2e__/"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@wdio/selenium-standalone-service": "^5.11.2",
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "css-loader": "^2.1.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@testing-library/react-hooks": "^2.0.1",
    "@wdio/eyes.webdriverio": "^2.0.5",
    "codeceptjs": "^2.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.3",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.18.11",
    "webdriverio": "^5.9.4",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
