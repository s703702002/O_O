{
  "presets": [
    ["@babel/env", {
        "targets": {
          "browsers": ["last 2 versions", "not ie < 11"]
        }
    }],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "spec": true }],
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-transform-runtime", {
        "regenerator": true
    }]
  ]
}
